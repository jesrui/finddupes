
gcc -Wall -O -g -I. finddupes.c md5/md5.o -o finddupes

clang -Wall -O -g -I. finddupes.c md5/md5.o -o finddupes

valgrind --leak-check=full ./finddupes testdir/recursed_b

valgrind --leak-check=full --gen-suppressions=all ./finddupes testdir/recursed_b

valgrind --tool=callgrind ./finddupes testdir/recursed_b

callgrind_annotate callgrind.out.8447

